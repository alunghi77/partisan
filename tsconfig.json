{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "files": [
        "scripts/contants.ts",
        "scripts/interfaces.d.ts",
        "scripts/models/aboutus.ts",
        "scripts/models/index.ts",
        "scripts/utils.ts",
        "typings/react/react-global.d.ts",
        "typings/react/react.d.ts",
        "typings/tsd.d.ts",
        "scripts/app.tsx",
        "scripts/components/cafebar.tsx",
        "scripts/components/contact.tsx",
        "scripts/components/howareweorganised.tsx",
        "scripts/components/howtogetinvolved.tsx",
        "scripts/components/inspiration.tsx",
        "scripts/components/whatispartisan.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
